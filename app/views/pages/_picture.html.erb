<div class="picture-container">
  <div class="row d-flex justify-content-center">
    <%= link_to 'Delete', post, method: :delete %>
  </div>
  <a href="<%=user_posts_path(post.user_id)%>" class="thumbnail"><%= image_tag post.picture.variant(resize_to_limit: [800,800]), class: "thumbnail picture" %></a>
</div>